!URXVT
URxvt.font: xft:Sauce\ Code\ Powerline:size=9
! URxvt.font: xft:SauceCodePro\ Nerd\ Font:size=9
URxvt.scrollBar: false
URxvt.letterSpace: -1.6
Uxvt.perl-ext-common:          default,clipboard
URxvt.keysym.Shift-Control-C:   perl:clipboard:copy
URxvt.keysym.Shift-Control-V:   perl:clipboard:paste
URxvt.clipboard.autocopy: true


!{{{ XFT
Xft.dpi:	92
Xft.antialias:	true
Xft.hinting:	true
Xft.autohint:	true
Xft.hintstyle:	hintfull
Xft.rgba:	rgb
!!}}}


! xautolock
Xautolock.notifier: notify-send Locking screen in 30s
Xautolock.notify: 30
Xautolock.corners: ----
Xautolock.time: 10
Xautolock.locker: xwobflock


